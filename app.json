{
  "expo": {
    "name": "Vivify Boilerplate",
    "description": "A very interesting project.",
    "slug": "development",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
