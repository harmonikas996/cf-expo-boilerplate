        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "dev-phir",
            "project": "expo-boilerplate",
            "authToken": "c3cb5c0c6ebd4e69b4aa727951403d23e22218da7ba04ab0b97c7455beed2410"
          }
        }
      ]
    },
    "android": {
      "useNextNotificationsApi": true
    } 
  }
}
